{
  "name": "@ignitial/dlake-service",
  "version": "3.0.3",
  "description": "Ignitial.io Data Lake service",
  "private": "true",
  "main": "server/index.js",
  "author": "Victor Andritoiu <devandrito@gmail.com>",
  "license": "MIT",
  "repository": "git@github.com:ignitialio/dlake-service.git",
  "scripts": {
    "lint": "eslint --ext .js index.js lib",
    "client:build": "vue-cli-service build",
    "client:serve": "vue-cli-service serve",
    "start:server": "docker-compose up -d mongo redis && node ./server/index.js",
    "start": "./start.sh",
    "dev": "npm-run-all --parallel start:server client:serve",
    "test:mongo": "./test/test-mongo.sh",
    "test:couch": "./test/test-couch.sh",
    "test:sentinel": "./test/test-couch.sh",
    "populate:mongo": "./test/populate-mongo.sh",
    "populate:couch": "./test/populate-couch.sh",
    "docker:build": "docker build --rm -t ignitial/dlake . && docker tag ignitial/dlake registry.ignitial.io/ignitial/dlake:${npm_package_version} && docker tag ignitial/dlake ignitial/dlake:${npm_package_version}",
    "docker:publish:private": "docker push registry.ignitial.io/ignitial/dlake && docker push registry.ignitial.io/ignitial/dlake:${npm_package_version}",
    "docker:publish:public": "docker push ignitial/dlake && docker push ignitial/dlake:${npm_package_version}"
  },
  "dependencies": {
    "@ignitial/iio-services": "^2.2.3",
    "accesscontrol": "^2.2.1",
    "bcryptjs": "^2.4.3",
    "core-js": "^2.6.9",
    "lodash": "^4.17.14",
    "mongodb": "^3.2.7",
    "nano": "^8.1.0",
    "pino": "^5.12.6"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.9.2",
    "@vue/cli-service": "^3.9.2",
    "chalk": "^2.4.2",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "got": "^9.6.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.22.5",
    "sass-loader": "^7.1.0",
    "should": "^13.2.3"
  }
}
